{
    "dashboard": {
        "id": null,
        "title": "Twilight Relayer Dashboard",
        "tags": [
            "relayer",
            "twilight",
            "trading"
        ],
        "style": "dark",
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "System Overview",
                "type": "row",
                "gridPos": {
                    "h": 1,
                    "w": 24,
                    "x": 0,
                    "y": 0
                },
                "collapsed": false
            },
            {
                "id": 2,
                "title": "CPU Usage",
                "type": "stat",
                "targets": [
                    {
                        "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
                        "refId": "A"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 1
                },
                "options": {
                    "reduceOptions": {
                        "values": false,
                        "calcs": [
                            "lastNotNull"
                        ],
                        "fields": ""
                    },
                    "orientation": "auto",
                    "textMode": "auto",
                    "colorMode": "value",
                    "graphMode": "area",
                    "justifyMode": "auto"
                },
                "pluginVersion": "8.0.0",
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "mappings": [],
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "red",
                                    "value": 80
                                }
                            ]
                        },
                        "unit": "percent"
                    }
                }
            },
            {
                "id": 3,
                "title": "Memory Usage",
                "type": "stat",
                "targets": [
                    {
                        "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
                        "refId": "A"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 1
                },
                "options": {
                    "reduceOptions": {
                        "values": false,
                        "calcs": [
                            "lastNotNull"
                        ],
                        "fields": ""
                    },
                    "orientation": "auto",
                    "textMode": "auto",
                    "colorMode": "value",
                    "graphMode": "area",
                    "justifyMode": "auto"
                },
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "mappings": [],
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                },
                                {
                                    "color": "red",
                                    "value": 80
                                }
                            ]
                        },
                        "unit": "percent"
                    }
                }
            },
            {
                "id": 4,
                "title": "Disk Usage",
                "type": "stat",
                "targets": [
                    {
                        "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\"} * 100) / node_filesystem_size_bytes{mountpoint=\"/\"})",
                        "refId": "A"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 12,
                    "y": 1
                },
                "options": {
                    "reduceOptions": {
                        "values": false,
                        "calcs": [
                            "lastNotNull"
                        ],
                        "fields": ""
                    },
                    "orientation": "auto",
                    "textMode": "auto",
                    "colorMode": "value",
                    "graphMode": "area",
                    "justifyMode": "auto"
                },
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "mappings": [],
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
