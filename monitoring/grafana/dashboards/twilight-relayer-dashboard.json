            {
                "id": 5,
                "title": "Network I/O",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(node_network_receive_bytes_total[5m])",
                        "refId": "A"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 18,
                    "y": 1
                },
                "options": {
                    "reduceOptions": {
                        "values": false,
                        "calcs": [
                            "lastNotNull"
                        ],
                        "fields": ""
                    },
                    "orientation": "auto",
                    "textMode": "auto",
                    "colorMode": "value",
                    "graphMode": "area",
                    "justifyMode": "auto"
                },
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "mappings": [],
                        "unit": "binBps"
                    }
                }
            },
